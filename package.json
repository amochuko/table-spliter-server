{
  "name": "tablespliter",
  "version": "1.0.0",
  "description": "A server for the table spliter app",
  "main": "src/main.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "start": "npm run build && node dist/main.js",
    "dev": "NODE_ENV=development nodemon",
    "test": "jest",
    "test:coverage": "jest --forceExit --coverage --verbose",
    "profiler": "NODE_ENV=production node --prof dist/main.js",
    "lint": "npx eslint .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "migrate:db": "NODE_ENV=development ts-node src/scripts/run-migrations.ts"
  },
  "author": "Ochuko <7204868+amochuko@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@parcel/watcher": "^2.5.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  }
}
